{
  "name": "testcafe-reports",
  "version": "0.0.1",
  "description": "Visual report to show success and errors of testcafe tests suite",
  "main": "index.js",
  "scripts": {
    "next": "next",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=development next build",
    "prebuild:prod": "npm run clean",
    "build:prod": "NODE_ENV=production next build",
    "clean": "rm -rf ./build ./_tmp ./_next ./_error .next",
    "dev": "NODE_ENV=development nodemon --inspect ./server/www.js",
    "local": "PORT=9000 HTTPS_PORT=9001 npm run dev",
    "lint": "eslint . --fix && stylelint './**/*.css' './**/*.scss' --fix",
    "postbuild": "next export -o ./_tmp && cp -r ./_tmp/* ./ && rm -rf ./_tmp/",
    "precommit": "lint-staged && npm test",
    "start": "node ./server/www.js",
    "test": "jest --verbose false --silent"
  },
  "nodemonConfig": {
    "watch": [
      "./server"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  },
  "engines": {
    "node": "10"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "_next"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "express": "^4.16.3",
    "file-loader": "^3.0.1",
    "lost": "^8.3.1",
    "next": "^7.0.3",
    "next-compose-plugins": "^2.1.1",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "node-sass": "4.11.0",
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "stylelint": "^9.9.0"
  }
}
